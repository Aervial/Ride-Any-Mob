{
	"schemaVersion": 1,
	"id": "ride-any-mob",
	"version": "${version}",
	"name": "Ride Any Mob",
	"description": "Ride any mob in Minecraft with this mod!",
	"authors": [
		"Aervial"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/ride-any-mob/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"aervial.rideanymob.RideAnyMob"
		],
		"client": [
			"aervial.rideanymob.RideAnyMobClient"
		]
	},
	"mixins": [
		"ride-any-mob.mixins.json",
		{
			"config": "ride-any-mob.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.18.4",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
